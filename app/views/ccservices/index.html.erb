<p id="notice"><%= notice %></p>

<h1>Ccservices</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Subtitle</th>
      <th>Body</th>
      <th>Slug</th>
      <th>Ccimg1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ccservices.each do |ccservice| %>
      <tr>
        <td><%= ccservice.title %></td>
        <td><%= ccservice.subtitle %></td>
        <td><%= ccservice.body %></td>
        <td><%= ccservice.slug %></td>
        <td><%= ccservice.ccimg1 %></td>
        <td><%= link_to 'Show', ccservice %></td>
        <td><%= link_to 'Edit', edit_ccservice_path(ccservice) %></td>
        <td><%= link_to 'Destroy', ccservice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ccservice', new_ccservice_path %>
